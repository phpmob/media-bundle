{
    "name": "phpmob/media-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "PhpMob\\MediaBundle\\": "src/"
        }
    },
    "require": {
        "php": "^7.1",

        "helios-ag/fm-elfinder-bundle": "^8",
        "league/flysystem-cached-adapter": "^1.0",
        "liip/imagine-bundle": "^1.6",
        "oneup/flysystem-bundle": "^2.0",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.0",
        "sylius/resource-bundle": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "phpspec/phpspec": "^4.0"
    },
    "suggest": {
        "doctrine/doctrine-cache-bundle": "To use Phpcr driver. Version: ^1.3",
        "doctrine/phpcr-bundle": "To use Phpcr driver. Version: ^1.3",
        "jackalope/jackalope-doctrine-dbal": "To use Phpcr driver. Version: ^1.3",
        "league/flysystem-phpcr": "To use Phpcr driver. Version: ^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
